var root  myname;
var folder downloads;
from { 
    url https://www.google.com/; 
}
retrieve { 
    img;
    a id mainLink;
    p;
    p id description;
    p id bio;
}
to { 
    myname/user/images;
    myname/user/downloads;
    myname/user/documents;
    myname/user/tla;
}
auth {
    username user;
    password myPassword;
}
